categorize
required_capability: categorize

# Drop the category ID, because it's non-deterministic
FROM sample_data
  | STATS count=COUNT(), values=MV_SORT(VALUES(message)) BY category=CATEGORIZE(message)
  | SORT count DESC, category ASC
  | DROP category
;

